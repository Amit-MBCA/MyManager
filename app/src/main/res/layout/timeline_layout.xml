<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!--    app:lineStyleDashGap="5dp"-->
    <!--    app:lineStyleDashLength="10dp"-->

    <com.baoyachi.stepview.VerticalStepView
        android:id="@+id/verticalsv"
        android:layout_width="66dp"
        android:layout_height="392dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="312dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.197" />

    <TextView
        android:id="@+id/row_timeline_layout_text_view_name"
        android:layout_width="78dp"
        android:layout_height="23dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="596dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/row_timeline_layout_text_view_description"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="124dp"
        android:layout_marginBottom="596dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/row_timeline_layout_text_view_name"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/row_timeline_layout_text_view_time"
        android:layout_width="146dp"
        android:layout_height="60dp"
        android:layout_marginTop="156dp"
        android:layout_marginEnd="120dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>